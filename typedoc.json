{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/actors/index.ts"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/tests/**",
    "**/node_modules/**",
    "**/*.test.tsx",
    "**/*.spec.tsx"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "skipErrorChecking": true,
  "readme": "./README.md",
  "plugin": [],
  "theme": "default",
  "navigationLinks": {
    "GitHub": "https://github.com/VaughnVernon/DomoActors"
  },
  "includeVersion": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Class",
    "Interface",
    "Enum",
    "Function",
    "TypeAlias",
    "Variable"
  ],
  "visibilityFilters": {
    "protected": true,
    "private": false,
    "inherited": true,
    "external": false
  },
  "searchInComments": true,
  "searchInDocuments": true
}
